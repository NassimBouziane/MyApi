{
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": { "project": ["./tsconfig.json"] },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-param-reassign": "off",
        // "import/no-named-as-default":0,
        // "import/no-import-module-exports":0
    }
}
